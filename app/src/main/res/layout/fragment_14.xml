<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BusinessFrame.Fragment14">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_below="@id/header"
        android:layout_width="match_parent"
        android:layout_height="437dp"
        android:clickable="true"
        android:focusable="true"
        android:background="@drawable/frame7">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true">

            <ImageView
                android:id="@+id/shopimageview"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="3dp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="48dp"
                android:layout_marginEnd="12dp"
                android:adjustViewBounds="true"
                android:clickable="true"
                android:visibility="visible"/>

            <TextView
                android:id="@+id/shopnametext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="100dp"
                android:layout_marginBottom="45dp"
                android:clickable="true"
                android:focusable="true"
                android:text="Shop Name"
                android:textColor="@color/white"
                android:textStyle="bold" />


                <ImageView
                    android:id="@+id/callImageView"
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:src="@drawable/ic_baseline_call_2421"
                    android:background="@drawable/circle_black_fill"
                    android:padding="3dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginStart="90dp"
                    android:layout_marginBottom="73dp"/>

                <TextView
                    android:id="@+id/shopcontactNumbertext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:clickable="true"
                    android:layout_alignParentBottom="true"
                    android:layout_toRightOf="@id/callImageView"
                    android:maxLines="1"
                    android:textSize="12dp"
                    android:text="0123456789"
                    android:textColor="@color/black"
                    android:layout_marginStart="0dp"
                    android:layout_marginBottom="72dp"/>

            <ImageView
                android:id="@+id/locationimageview"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="22dp"
                android:padding="3dp"
                android:background="@drawable/circle_white_fill"
                android:src="@drawable/ic_outline_location_brown_24"
                android:textColor="@color/black" />


            <TextView
                android:id="@+id/shopownernametext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_toRightOf="@id/locationimageview"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="21dp"
                android:clickable="true"
                android:maxLines="1"
                android:textSize="12dp"
                android:text="owner Name"
                android:textColor="@color/white" />

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_gravity="bottom"
                android:textColor="@color/TextColor"
                android:textSize="20dp"
                android:clickable="true"
                android:visibility="visible"/>

            <TextView
                android:id="@+id/text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_gravity="center_vertical"
                android:textColor="@color/TextColor"
                android:textSize="20dp"
                android:clickable="true"
                android:visibility="visible"/>

            <LinearLayout
                android:id="@+id/edittextlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@color/white"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/cancelimage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_baseline_clear_24"/>

                <EditText
                    android:id="@+id/texteditor"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="8dp"
                    android:paddingTop="8dp"
                    android:paddingEnd="8dp"
                    android:paddingBottom="8dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_gravity="center"
                    android:drawablePadding="8dp"
                    android:background="@color/white"/>

                <ImageView
                    android:id="@+id/checkimage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_baseline_check_24"/>

            </LinearLayout>

        </RelativeLayout>
    </FrameLayout>


</FrameLayout>